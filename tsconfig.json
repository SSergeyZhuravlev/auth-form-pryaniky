{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "lib": ["ESNext", "DOM"],
        "target": "ES2017",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true
    }
  }